   "source": [
    "x = int(input(\"Enter the value of x: \"))\n",
    "\n",
    "match x:\n",
    "    case 0:\n",
    "        print(\"x is zero\")\n",
    "    case 4:\n",
    "        print(\"x is 4\")\n",
    "    case _ if x != 90:\n",
    "        print(x, \"is not 90\")\n",
    "    case _ if x != 80:\n",
    "        print(x, \"is not 80\")\n",
    "    case _:\n",
    "        print(\"Default case: x =\", x)\n"
   ]
